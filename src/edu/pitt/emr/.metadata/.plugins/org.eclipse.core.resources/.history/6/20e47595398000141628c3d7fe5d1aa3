package edu.pitt.emr;

public class Diagnoses {

    /**
     * This class is for the classifications of a Diagnoses
     * 
     */

    private int diagnosesID;
    private String name;

    public Diagnoses(int diagnosesID, String name) {
        this.diagnosesID = diagnosesID;
        this.name = name;

    } // end of Diagnoses method

    /**
     * Getters and Setters for the Diagnoses Class
     * 
     * @return
     */

    public int getDiagnosesID() {
        return diagnosesID;
    }

    public void setDiagnosesID(int diagnosesID) {
        this.diagnosesID = diagnosesID;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

} // end of Diagnoses class
