package edu.pitt.emr;

public class Medication {

    /**
     * This class is for the classifications of a Medication
     * 
     */

    private int medicationID;
    private String name;

    public Medication(int medicationID, String name) {
        this.medicationID = medicationID;
        this.name = name;

    } // end of Medication method

    /**
     * Getters and setters for Medication
     */

    public int getMedicationID() {
        return medicationID;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getName() {
        return name;
    }

}// end of Medication class
